<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="462.0" prefWidth="639.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.impl.multiplayer.RoundEndController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label alignment="TOP_CENTER" prefHeight="116.0" prefWidth="289.0" text="Round Over">
        <font>
            <Font size="36.0"/>
        </font>
    </Label>
    <Label fx:id="lblServerAns" alignment="TOP_CENTER" prefHeight="59.0" prefWidth="537.0"
           text="Waiting For Round To Be Over..." wrapText="true">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <Label fx:id="lblUserAns" alignment="TOP_CENTER" prefHeight="61.0" prefWidth="537.0" text="You Answered..."
           wrapText="true">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <Label fx:id="lblPoints" text="POINTS">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <TableView fx:id="tablePlayers" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="293.0" text="Name"/>
            <TableColumn fx:id="columnPoints" prefWidth="305.0" text="Points"/>
        </columns>
    </TableView>
</VBox>
