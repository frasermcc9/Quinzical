<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.impl.controllers.IntroController">
    <AnchorPane prefHeight="749.0" prefWidth="1227.0" style="-fx-background-color: #232323;">
        <ImageView fx:id="imgTitle" fitHeight="363.0" fitWidth="908.0" layoutX="126.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../../images/title.png" />
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
        </ImageView>
        <Button fx:id="btnPractice" layoutX="407.0" layoutY="539.0" mnemonicParsing="false" onAction="#btnPracticeClick" prefHeight="149.0" prefWidth="414.0" style="-fx-background-color: #30ff83; -fx-background-radius: 20;" text="PRACTICE">
            <font>
                <Font name="Segoe UI Semibold" size="65.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="btnPlay" layoutX="375.0" layoutY="323.0" mnemonicParsing="false" onAction="#btnPlayClick" prefHeight="172.0" prefWidth="478.0" style="-fx-background-color: #fcba03; -fx-background-radius: 20;" text="PLAY">
            <font>
                <Font name="Segoe UI Semibold" size="65.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <cursor>
            <Cursor fx:constant="DEFAULT" />
        </cursor>
    </AnchorPane>
</Scene>
