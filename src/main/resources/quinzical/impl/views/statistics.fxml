    <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" style="-fx-spacing: 25;" styleClass="vbox" stylesheets="@../../../css/intro.css">
        <Label prefHeight="148.0" prefWidth="639.0" styleClass="title" stylesheets="@../../../css/title-label.css" text="Statistics">
            <font>
                <Font size="61.0" />
            </font>
            <VBox.margin>
