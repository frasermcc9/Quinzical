<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.impl.controllers.PracticeQuestionController">
    <AnchorPane fx:id="background" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #232323;">
      <ImageView fx:id="imgBackground" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imgOverlay" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/overlays/gameQuestion.png" />
         </image>
      </ImageView>
        <Pane fx:id="panePracticeSolutions" layoutX="346.0" layoutY="230.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="277.0" prefWidth="589.0" stylesheets="@../../../css/intro.css" />
      <Label fx:id="lblPrompt" alignment="TOP_RIGHT" layoutX="273.0" layoutY="192.0" prefHeight="31.0" prefWidth="737.0" stylesheets="@../../../css/game-question.css" text="Label" />
      <Button fx:id="btnSubmit" layoutX="532.0" layoutY="622.0" mnemonicParsing="false" onAction="#onSubmitClicked" prefHeight="65.0" prefWidth="220.0" text="Submit">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <stylesheets>
            <URL value="@../../../css/intro.css" />
            <URL value="@../../../css/game-question.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="transparent" />
            <String fx:value="submit-button" />
         </styleClass>
      </Button>
      <Button fx:id="btnPass" layoutX="782.0" layoutY="622.0" mnemonicParsing="false" onAction="#onPassClicked" prefHeight="65.0" prefWidth="220.0" text="Pass">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <stylesheets>
            <URL value="@../../../css/intro.css" />
            <URL value="@../../../css/game-question.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="transparent" />
            <String fx:value="pass-button" />
         </styleClass>
      </Button>
      <ImageView fitHeight="47.0" fitWidth="37.0" layoutX="624.0" layoutY="560.0" onMouseReleased="#onReplyClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/replay.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label layoutX="576.0" layoutY="533.0" styleClass="label-replay" stylesheets="@../../../css/game-question.css" text="Repeat Question" />
      <Button fx:id="btnBack" layoutX="279.0" layoutY="622.0" mnemonicParsing="false" onAction="#onBackClicked" prefHeight="65.0" prefWidth="220.0" text="Back">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <stylesheets>
            <URL value="@../../../css/intro.css" />
            <URL value="@../../../css/game-question.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="transparent" />
            <String fx:value="pass-button" />
         </styleClass>
      </Button>
      <Label fx:id="lblHint" alignment="TOP_RIGHT" layoutX="272.0" layoutY="123.0" prefHeight="92.0" prefWidth="737.0" stylesheets="@../../../css/game-question.css" text="Label" />
      <Label fx:id="lblAttempts" alignment="TOP_RIGHT" layoutX="273.0" layoutY="503.0" prefHeight="46.0" prefWidth="737.0" stylesheets="@../../../css/game-question.css" text="Label" />
        <cursor>
            <Cursor fx:constant="DEFAULT" />
        </cursor>
    </AnchorPane>
</Scene>
